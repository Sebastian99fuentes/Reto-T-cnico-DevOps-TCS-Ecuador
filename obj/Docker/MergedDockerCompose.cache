name: dockercompose5993307539421813490
services:
  api:
    build:
      context: C:\Users\sebas\Programas\PruebaDevops\PruebaDevops
      dockerfile: Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
    expose:
      - "80"
    healthcheck:
      test:
        - CMD
        - curl
        - -f
        - http://localhost:80/health
      timeout: 10s
      interval: 30s
      retries: 3
      start_period: 40s
    image: api
    networks:
      app-network: null
  ocelotgateway:
    build:
      context: C:\Users\sebas\Programas\PruebaDevops\PruebaDevops
      dockerfile: OcelotGateway/Dockerfile
    depends_on:
      api:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: ocelotgateway
    networks:
      app-network: null
    ports:
      - mode: ingress
        target: 8080
        published: "8010"
        protocol: tcp
networks:
  app-network:
    name: dockercompose5993307539421813490_app-network
    driver: bridge